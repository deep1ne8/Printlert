{
  "name": "printlert",
  "version": "1.0.0",
  "description": "A production-ready printer supply monitoring portal with SNMP capabilities, real-time alerts, and comprehensive management features",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "serve": "npx http-server -p 8000",
    "build": "npm run build:client",
    "build:client": "echo 'Client build completed'",
    "test": "jest",
    "lint": "eslint .",
    "security": "npm audit",
    "production": "NODE_ENV=production node server.js"
  },
  "keywords": [
    "printer",
    "monitoring",
    "snmp",
    "toner",
    "supply",
    "dashboard",
    "alerts",
    "network",
    "management",
    "web-app",
    "production",
    "enterprise"
  ],
  "author": "Printlert Team",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "express-rate-limit": "^7.1.5",
    "net-snmp": "^1.2.4",
    "nodemailer": "^6.9.7",
    "twilio": "^4.19.0",
    "winston": "^3.11.0",
    "compression": "^1.7.4",
    "express-validator": "^7.0.1",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "dotenv": "^16.3.1",
    "pm2": "^5.3.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
