{
  "name": "printlert",
  "version": "1.0.0",
  "description": "A production-ready printer supply monitoring portal with SNMP capabilities, real-time alerts, and comprehensive management features",
  "main": "server.js",
  "bin": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "serve": "npx http-server -p 8000",
    "build": "npm run build:client",
    "build:client": "echo 'Client build completed'",
    "build:exe:win": "pkg . --targets node18-win-x64 --out-path dist",
    "build:exe:all": "pkg . --targets node18-win-x64,node18-linux-x64,node18-macos-x64 --out-path dist",
    "test": "jest",
    "lint": "eslint .",
    "security": "npm audit",
    "production": "NODE_ENV=production node server.js"
  },
  "keywords": [
    "printer",
    "monitoring",
    "snmp",
    "toner",
    "supply",
    "dashboard",
    "alerts",
    "network",
    "management",
    "web-app",
    "production",
    "enterprise"
  ],
  "author": "Printlert Team",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.11.0",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "net-snmp": "^1.2.4",
    "nodemailer": "^6.9.7",
    "pm2": "^5.3.0",
    "prompt-sync": "^4.2.0",
    "twilio": "^4.19.0",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "eslint": "^8.55.0",
    "http-server": "^14.1.1",
    "jest": "^29.7.0",
    "live-server": "^1.2.2",
    "nodemon": "^3.0.2",
    "pkg": "^5.8.1"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "pkg": {
    "assets": [
      "public/**/*",
      "healthcheck.js",
      "node_modules/axios/**/*"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/deep1ne8/Printlert.git"
  },
  "bugs": {
    "url": "https://github.com/deep1ne8/Printlert/issues"
  },
  "homepage": "https://github.com/deep1ne8/Printlert#readme"
}
